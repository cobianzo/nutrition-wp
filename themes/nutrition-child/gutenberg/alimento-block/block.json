{
  "apiVersion": 3,
  "name": "asim/alimento-block",
  "title": "Block Alimento",
  "category": "widgets",
  "icon": "smiley",
  "supports": {
    "align": false,
    "anchor": true,
    "html": false
  },
  "textdomain": "asim",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "textContent": {
      "type": "string",
      "source": "html",
      "selector": ".alimento-left-column"
    },
    "alimentoID": {
      "type": "string",
      "default": ""
    },
    "imgSrc": {
      "type": "string",
      "default": ""
    },
    "mealType": {
      "type": "string",
      "default": ""
    }
  },
  "providesContext": {
    "custom/restricted-inner-blocks": "content"
  },
  "usesContext": ["core/paragraph", "core/heading"],

  "editorScript": "file:../../build/alimento-block.js",
  "style": "file:./style.css",
  "render": "file:./render.php"
}
